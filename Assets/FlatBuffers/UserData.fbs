namespace BanpoFri.Data;



table UserData {
// @AutoCreate DataClass
  money:double; // @ReactiveProperty
  cash:int = 0;// @ReactiveProperty
  stageidx:int = 0; // @ReactiveProperty
  buyinappids:string;
  tutorial:[string];
  lastlogintime:long = 0;
  optiondata:OptionData;
  recordcount:[RecordCount];
  gamestarttime:long = 0;
  uuid:long = 0;
  recordvalue:[RecordCount];
  testmoney:int;
  ordertest:[int];
}

table OptionData {
  language:string;
  bgm:bool = false;
  effect:bool = false;
  slowgraphic:bool = false;
  vibration:bool = true;
  subscribeorder:bool = false;
  autofelling:bool = false;
}

table RecordCount {
  idx:string;
  count:int = 0;
}


root_type UserData;
